{
  "name": "sapdemoapp",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@cap-js/hana": "^1",
    "@sap-cloud-sdk/http-client": "^3.22.2",
    "@sap-cloud-sdk/resilience": "^3.22.2",
    "@sap/cds": "^8",
    "@sap/cds-compiler": "^5.4.2",
    "@sap/cds-odata-v2-adapter-proxy": "^1.9.21",
    "@sap/xssec": "^3",
    "@sap/approuter": "^10",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@cap-js/cds-types": "^0.7.0",
    "@cap-js/sqlite": "^1",
    "@sap/cds-dk": "^8",
    "@sap/ux-specification": "UI5-1.130",
    "rimraf": "^5.0.5"
  },
  "scripts": {
    "start": "cds-serve",
    "watch-app1": "cds watch --open app1/webapp/index.html?sap-ui-xx-viewCache=false"
  },
  "cds": {
    "sql": {
      "native_hana_associations": false
    },
    "requires": {
      "hana": {
        "deploy-format": "hdbtable"
      },
      "db": {
        "kind": "sqlite",
        "model": "*",
        "credentials": {
          "database": "mydb.db"
        }
      },
      "destinations": true,
      "API_PLANT_SRV": {
        "kind": "odata-v2",
        "model": "srv/external/API_PLANT_SRV",
        "[sandbox]": {
          "credentials": {
            "path": "/sap/opu/odata/sap/ZAPI_PLANT",
            "destination": "my403232_Public_cloud",
            "forwardAuthToken": true
          }
        },
        "[hybrid]": {
          "credentials": {
            "path": "/sap/opu/odata/sap/ZAPI_PLANT",
            "destination": "my403232_Public_cloud",
            "forwardAuthToken": true
          }
        },
        "[production]": {
          "credentials": {
            "path": "/sap/opu/odata/sap/ZAPI_PLANT",
            "destination": "my403232_Public_cloud",
            "forwardAuthToken": true
          }
        }
      },
      "API_EQUIPMENT": {
        "kind": "odata-v2",
        "model": "srv/external/API_EQUIPMENT",
        "csrf": true,
        "csrfInBatch": true,
        "[sandbox]": {
          "credentials": {
            "path": "/sap/opu/odata/sap/API_EQUIPMENT",
            "destination": "my403232_Public_cloud",
            "forwardAuthToken": true
          }
        },
        "[production]": {
          "credentials": {
            "path": "/sap/opu/odata/sap/API_EQUIPMENT",
            "destination": "my403232_Public_cloud",
            "forwardAuthToken": true
          }
        },
        "[hybrid]": {
          "credentials": {
            "path": "/sap/opu/odata/sap/API_EQUIPMENT",
            "destination": "my403232_Public_cloud",
            "forwardAuthToken": true
          }
        }
      }
    }
  },
  "sapux": [
    "app/app1"
  ]
}
